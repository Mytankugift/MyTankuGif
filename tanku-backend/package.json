{
  "name": "tanku-backend",
  "version": "1.0.0",
  "description": "Backend propio de Tanku - E-commerce con red social",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "prisma generate && tsc",
    "start": "npm run prisma:migrate:deploy && node dist/app.js",
    "start:workers": "node dist/workers.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test:jobs": "tsx src/modules/dropi-jobs/test-jobs.ts",
    "workers:start": "tsx src/modules/dropi-jobs/workers/start-workers.ts",
    "migrate:db": "tsx migrate-database.ts",
    "check:migrations": "tsx check-migrations.ts",
    "fix:feed:tables": "tsx scripts/create-feed-tables.ts",
    "init:feed": "tsx scripts/init-feed-metrics.ts",
    "clean:migrations": "tsx scripts/clean-failed-migrations.ts"
  },
  "keywords": [
    "ecommerce",
    "social-network",
    "api",
    "typescript"
  ],
  "author": "Tanku Team",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@tanstack/react-query": "^5.90.15",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.0.1",
    "framer-motion": "^12.24.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.3",
    "winston": "^3.15.0",
    "zod": "^3.25.76",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.0",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "prisma": "^7.2.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
